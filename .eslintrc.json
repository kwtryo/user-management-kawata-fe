{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
        "react-app",
        "react-app/jest"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "space-before-function-paren": "off",
        "@typescript-eslint/space-before-function-paren": "off"
    }
}
